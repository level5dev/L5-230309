{
  "components/custom/custom-fs": "<nav class=\"custom-fs__nav\">\r\n    {{#any facets visible=true}}\r\n    <ul class=\"custom-fs__list\">\r\n        <li class=\"custom-fs__list-item\">\r\n            <a data-value=\"__product-tag=all\" class=\"button button--facet {{#unless facets.[0].selected}} button--primary {{/unless}}\">All</a>\r\n        </li>\r\n        {{#each facets}}\r\n        {{#if this.facet '===' '__product-tag'}}\r\n        {{#each items}}\r\n        <li class=\"custom-fs__list-item\">\r\n            <a data-value=\"{{param_name}}={{param_value}}\"class=\"button button--facet {{#if selected}} button--primary is-selected{{/if}}\" rel=\"nofollow\">\r\n                {{{title}}}\r\n            </a>\r\n        </li>\r\n        {{/each}} \r\n        {{/if}}\r\n        {{/each}}\r\n    </ul>\r\n    {{/any}}\r\n</nav>\r\n\r\n<form class=\"actionBar category-facets\" method=\"get\" data-sort-by=\"product\">\r\n    <fieldset class=\"form-fieldset actionBar-section\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label\" for=\"sort\">Type:</label>\r\n            <select class=\"form-select form-select--small categoryPage\" name=\"__product-tag\" id=\"sort\" role=\"listbox\">\r\n                <option value=\"all\" {{#if selected \"===\" false}} selected{{/if}}>All</option>\r\n                {{#each facets}}\r\n                {{#if this.facet '===' '__product-tag'}}\r\n                {{#each items}}\r\n                <option value=\"{{category.url}}{{param_value}}\" {{#if selected}}selected{{/if}}>{{title}}</option>\r\n                {{/each}}\r\n                {{/if}}\r\n            {{/each}}\r\n            </select>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n"
}
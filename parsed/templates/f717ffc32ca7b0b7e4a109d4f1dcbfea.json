{
  "components/custom/home-bottom-callout": "<section class=\"container home-bottom-callout\">\r\n    \r\n    <div class=\"home-bottom-callout__container container\">\r\n        <!-- giveaway -->\r\n        <article class=\"home-bottom-card home-bottom-card--giveaway\" >\r\n            <div class=\"home-bottom-card__content\">\r\n                <h1 class=\"home-bottom-card__heading h2\">\r\n                    {{~lang 'custom.home.bottom-callout.callout-1.title'~}}\r\n                </h1>\r\n                <p class=\"home-bottom-card__text\">\r\n                    <span class=\"home-bottom-card__text-month\">\r\n                        {{lang 'custom.home.bottom-callout.callout-1.month'}}\r\n                    </span>\r\n\r\n                    <br>\r\n\r\n                    <span class=\"home-bottom-card__text-winner h3\">\r\n                        {{lang 'custom.home.bottom-callout.callout-1.winner-name'}}\r\n                    </span>\r\n\r\n                    <br>\r\n\r\n                    {{lang 'custom.home.bottom-callout.callout-1.location'}}\r\n                </p>\r\n            </div>\r\n            <div class=\"home-bottom-card__item\">\r\n                <div class=\"home-bottom-card__item-content\">\r\n                    <h2 class=\"home-bottom-card__item-heading h4\">\r\n                        {{~lang 'custom.home.bottom-callout.callout-1.item-month'~}}\r\n                    </h2>\r\n                    <div class=\"home-bottom-card__item-wrapper\">\r\n                        <div class=\"home-bottom-card__item-image\">\r\n                            <img src=\"{{cdn (concat 'webdav:' (lang 'custom.home.bottom-callout.callout-1.item-image'))}}\"\r\n                                alt=\"{{lang 'custom.home.bottom-callout.callout-1.item-image-alt'}}\">\r\n                        </div>\r\n                        <div class=\"home-bottom-card__item-text\">\r\n                            <div>\r\n                                <img src=\"{{cdn (concat 'webdav:' (lang 'custom.home.bottom-callout.callout-1.item-brand-img'))}}\"\r\n                                    alt=\"{{lang 'custom.home.bottom-callout.callout-1.item-brand-img-alt'}}\">\r\n                            </div>\r\n                            <p>\r\n                                <span>\r\n                                    {{lang 'custom.home.bottom-callout.callout-1.item-title'}}\r\n                                </span>\r\n                                <br>\r\n                                {{{lang 'custom.home.bottom-callout.callout-1.item-value'}}}\r\n                            </p>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n                <p class=\"home-bottom-card__link\">\r\n                    {{lang 'custom.home.bottom-callout.callout-1.link-text'}}\r\n                    <a  href=\"{{lang 'custom.home.bottom-callout.callout-1.link-url'}}\" >\r\n                        Join today!\r\n                    </a> \r\n                </p>\r\n            </article>\r\n\r\n        <!-- VIP Registration -->\r\n        <article class=\"bg-paint-splatter home-bottom-card home-bottom-card--vip\" >\r\n            <div class=\"home-bottom-card__content\">\r\n                <h1 class=\"home-bottom-card__heading h2\">\r\n                    {{~lang 'custom.home.bottom-callout.callout-2.title'~}}\r\n                </h1>\r\n                <ul class=\"home-bottom-card__list\">\r\n                    <li class=\"h3\">{{{lang 'custom.home.bottom-callout.callout-2.bullet-1'}}}</li>\r\n                    <li class=\"h3\">{{{lang 'custom.home.bottom-callout.callout-2.bullet-2'}}}</li>\r\n                    <li class=\"h3\">{{{lang 'custom.home.bottom-callout.callout-2.bullet-3'}}}</li>\r\n                </ul>\r\n            </div>\r\n            <div class=\"home-bottom-card__newsletter\">\r\n                {{#if settings.show_newsletter_box}}\r\n                {{> components/common/subscription-form}}\r\n                {{/if}}\r\n            </div>\r\n                <p class=\"home-bottom-card__link\">\r\n                    {{{lang 'custom.home.bottom-callout.callout-2.details-text'}}}\r\n                </p>\r\n            </article>\r\n\r\n        <!-- Blog -->\r\n        <a class=\"home-bottom-card home-bottom-card--blog\" href=\"{{lang 'custom.home.bottom-callout.callout-3.button-link'}}\">\r\n            <article >\r\n                <h1 class=\"home-bottom-card__heading h2\">{{lang 'custom.home.bottom-callout.callout-3.title'}}</h1>\r\n                <img class=\"home-bottom-card__image\"\r\n                    src=\"{{cdn (concat 'webdav:' (lang 'custom.home.bottom-callout.callout-3.image'))}}\"\r\n                    alt=\"{{lang 'custom.home.bottom-callout.callout-3.image-alt'}}\">\r\n                <div class=\"home-bottom-card__content\">\r\n                    <div class=\"home-bottom-card__icon-wrapper\">\r\n                        <span class=\"icon icon--youtube\">\r\n                            <svg>\r\n                                <use xlink:href=\"#icon-youtube\"></use>\r\n                            </svg>\r\n                        </span>\r\n                    </div>\r\n                    <div class=\"home-bottom-card__text\">\r\n                        <h2 class=\"home-bottom-card__text-heading h3\">{{lang\r\n                            'custom.home.bottom-callout.callout-3.heading'}}</h2>\r\n                        <p class=\"home-bottom-card__text-desc\">{{lang 'custom.home.bottom-callout.callout-3.text'}}</p>\r\n\r\n                    </div>\r\n                </div>\r\n            </article>\r\n        </a>\r\n\r\n    </div>\r\n</section>\r\n",
  "components/common/subscription-form": "<h3 class=\"footer-info-heading\">{{lang 'newsletter.subscribe'}}</h3>\r\n<p class=\"footer-info-text\">{{lang 'newsletter.subscribe_intro'}}</p>\r\n\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <input class=\"form-input\"\r\n                       id=\"nl_email\"\r\n                       name=\"nl_email\"\r\n                       type=\"email\"\r\n                       value=\"\"\r\n                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\r\n                       aria-describedby=\"alertBox-message-text\"\r\n                       aria-required=\"true\"\r\n                       required\r\n                >\r\n                <button class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\">{{lang 'newsletter.subscribe_submit'}}</button>\r\n            </div>\r\n            {{#if settings.show_newsletter_summary }}\r\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}}\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n"
}
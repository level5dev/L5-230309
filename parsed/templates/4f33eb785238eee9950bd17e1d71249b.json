{
  "components/custom/freq-bought-together-image-item": "<li class=\"fbt__image-item isChecked\" data-product-id=\"{{product.id}}\">\r\n  <a class=\"fbt__image-link\" href=\"{{product.url}}\">\r\n    {{#if is_ajax}}\r\n      <img class=\"fbt__image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage product.main_image 'gallery_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{product.image.alt}}\" title=\"{{product.image.alt}}\">\r\n    {{else}}\r\n      <img class=\"fbt__image lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage product.image 'gallery_size' (cdn theme_settings.default_image_product)}}\" alt=\"{{product.image.alt}}\" title=\"{{product.image.alt}}\">\r\n    {{/if}}\r\n  </a>\r\n</li>\r\n"
}
{
  "components/custom/paginator-dropdown": "<nav class=\"pagination pagination--dropdown\" aria-label=\"pagination\">\r\n    <ul class=\"pagination-list\">\r\n        <li class=\"pagination-item pagination-item--previous\">\r\n            {{#if previous}}\r\n                <a class=\"pagination-link\"\r\n                   href=\"{{previous}}\"\r\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n                   aria-label=\"{{lang 'common.previous'}}\"\r\n                >\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-left\"></use>\r\n                        </svg>\r\n                    </i>\r\n                    {{lang 'common.previous'}}\r\n                </a>\r\n            {{/if}}\r\n        </li>\r\n\r\n        {{!-- Sets a variable to find and replace the page numbers in the select dropdown below --}}\r\n\r\n        {{#each (split (last (split links.[0].url '?')) '&')}}\r\n            {{#contains this \"page=\"}}\r\n                {{assignVar 'replace_text' this}}\r\n            {{/contains}}\r\n        {{/each}}\r\n\r\n        <li class=\"pagination-item pagination-item--dropdown\">\r\n            <label class=\"pagination-item__label\" for=\"paginate-pages\">{{lang 'custom.other.paginator.label'}}</label>\r\n            <select class=\"pagination-item__dropdown\" id=\"paginate-pages\" name=\"paginate-pages\" onchange=\"this.options[this.selectedIndex].value && (window.location = this.options[this.selectedIndex].value);\">\r\n                {{#for 1 page_count}}\r\n                    <option value=\"{{#replace (getVar 'replace_text') ../links.[0].url}}page={{$index}}{{/replace}}\" {{#unless reload}}data-faceted-search-facet{{/unless}} {{#if $index '==' ../current}}selected aria-current=\"page\" data-pagination-current-page-link {{/if}} >{{$index}}</option>\r\n                {{/for}}\r\n            </select>\r\n            <span class=\"pagination-item__page-count\">{{lang 'custom.other.paginator.of-label' page_count=page_count}}</span>\r\n        </li>\r\n\r\n        <li class=\"pagination-item pagination-item--next\">\r\n            {{#if next}}\r\n                <a class=\"pagination-link\"\r\n                   href=\"{{next}}\"\r\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n                   aria-label=\"{{lang 'common.next'}}\"\r\n                >\r\n                    {{lang 'common.next'}}\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-right\"></use>\r\n                        </svg>\r\n                    </i>\r\n                </a>\r\n            {{/if}}\r\n        </li>\r\n    </ul>\r\n</nav>\r\n"
}
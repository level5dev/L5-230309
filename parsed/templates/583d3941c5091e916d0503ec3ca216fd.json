{
  "components/products/description-tabs": "<div class=\"tabs-wrapper\">\r\n    <ul class=\"container tabs\" data-tab>\r\n        {{#if product.description}}\r\n            <li class=\"tab is-active\">\r\n                <a class=\"tab-title\" href=\"#tab-description\">{{lang 'products.description'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if product.warranty}}\r\n            <li class=\"tab {{#unless product.description}}is-active{{/unless}} tab--warranty\">\r\n                <a class=\"tab-title\" href=\"#tab-warranty\">{{lang 'products.warranty'}}</a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</div>\r\n\r\n<div class=\"tabs-contents\">\r\n    {{#if product.description}}\r\n        <div class=\"tab-content tab-content--description is-active\" id=\"tab-description\">\r\n            <div class=\"container\">\r\n                {{{product.description}}}\r\n            </div>\r\n        </div>\r\n    {{/if}}\r\n\r\n    {{#if product.warranty}}\r\n        <div class=\"container tab-content tab-content--warranty {{#unless product.description}}is-active{{/unless}}\" id=\"tab-warranty\">\r\n            {{{product.warranty}}}\r\n        </div>\r\n    {{/if}}\r\n</div>\r\n"
}
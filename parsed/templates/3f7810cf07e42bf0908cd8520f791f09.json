{
  "components/custom/category-cta": "<section class=\"container category-cta\">\r\n    <h2 class=\"category-cta-heading h1\">\r\n        {{lang 'custom.home.category-cta.heading'}}\r\n    </h2>\r\n    <div class=\"category-cta__container\">\r\n        {{#for 1 (lang 'custom.home.category-cta.number-of-ctas')}}\r\n            <article class=\"category-card\">\r\n                <figure class=\"category-card__figure\">\r\n                    <img src=\"{{cdn (concat 'webdav:' (lang (concat (concat 'custom.home.category-cta.cta-'$index)'.image')))}}\" alt=\"{{lang (concat (concat 'custom.home.category-cta.cta-'$index)'.image-alt')}}\">\r\n                </figure>\r\n                <div class=\"category-card__content\">\r\n                    <div class=\"category-card__content-container\">\r\n                        <h1 class=\"category-card__heading h2\">\r\n                            {{lang (concat (concat 'custom.home.category-cta.cta-'$index)'.title')}}\r\n                        </h1>\r\n                        <p class=\"category-card__text\">{{lang (concat (concat 'custom.home.category-cta.cta-'$index)'.text')}}</p>\r\n                    </div>\r\n                    <a href=\"{{lang (concat (concat 'custom.home.category-cta.cta-'$index)'.button-link')}}\" class=\"button button--primary category-card__button\">{{lang (concat (concat 'custom.home.category-cta.cta-'$index)'.button-text')}}</a>\r\n                </div>\r\n            </article>\r\n        {{/for}}\r\n    </div>\r\n</section>\r\n"
}
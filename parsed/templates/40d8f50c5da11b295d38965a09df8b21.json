{
  "components/common/body": "<main class=\"body\" id=\"main-content\" role=\"main\" data-currency-code=\"{{currency_selector.active_currency_code}}\">\r\n    {{#block \"hero\"}} {{/block}}\r\n\r\n\r\n    <div class=\"{{#unless container_active false}}container{{/unless}}\">\r\n        {{#block \"page\"}} {{/block}}\r\n    </div>\r\n\r\n    {{> components/common/modal/modal}}\r\n    {{> components/common/alert/alert-modal}}\r\n\r\n    {{#and (unless customer) (if theme_settings.its.popup_login)}}\r\n        {{> components/custom/popup-login-window-modal }}\r\n    {{/and}}\r\n</main>\r\n",
  "components/common/alert/alert-modal": "<div id=\"alert-modal\" class=\"modal modal--alert modal--small\" data-reveal data-prevent-quick-search-close>\r\n    <div class=\"swal2-icon swal2-error swal2-icon-show\"><span class=\"swal2-x-mark\"><span class=\"swal2-x-mark-line-left\"></span><span class=\"swal2-x-mark-line-right\"></span></span></div>\r\n\r\n    <div class=\"modal-content\"></div>\r\n\r\n    <div class=\"button-container\"><button type=\"button\" class=\"confirm button\" data-reveal-close>OK</button></div>\r\n</div>\r\n",
  "components/custom/popup-login-window-modal": "<div id=\"popupLoginWindow\" class=\"modal modal--small modal--popupLoginWindow\" data-reveal>\r\n\t<div class=\"modal-header\">\r\n\t\t<h2 class=\"modal-header-title\">{{lang 'common.login'}}</h2>\r\n\t\t{{> components/common/modal/modal-close-btn }}\r\n\t</div>\r\n\t<div class=\"modal-body\">\r\n\t\t{{> components/common/alert/alert-error (lang 'custom.other.popup-login.error')}}\r\n\t\t{{> components/common/alert/alert-success (lang 'custom.other.popup-login.success')}}\r\n\t\t<form class=\"form\" id=\"popupLoginWindowForm\">\r\n\t\t\t<div class=\"form-field\">\r\n\t\t\t\t<label class=\"form-label\" for=\"login_email\">{{lang 'login.field_email'}}</label>\r\n\t\t\t\t<input class=\"form-input\" name=\"login_email\" id=\"login_email\" type=\"email\">\r\n\t\t\t</div>\r\n\t\t\t<div class=\"form-field\">\r\n\t\t\t\t<label class=\"form-label\" for=\"login_pass\">{{lang 'login.field_password'}}</label>\r\n\t\t\t\t<input class=\"form-input\" id=\"login_pass\" type=\"password\" name=\"login_pass\">\r\n\t\t\t</div>\r\n\t\t\t<div class=\"form-actions\">\r\n\t\t\t\t<input type=\"submit\" class=\"button button--primary\" value=\"{{lang 'login.submit_value' }}\">\r\n\t\t\t\t<a class=\"button button--secondary\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\r\n\t\t\t\t<a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password' }}</a>\r\n\t\t\t</div>\r\n\t\t</form>\r\n\t</div>\r\n\t<div class=\"loadingOverlay\" style=\"display: none;\"></div>\r\n</div>\r\n",
  "components/common/modal/modal": "<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\r\n    {{> components/common/modal/modal-close-btn}}\r\n    <div class=\"modal-content\"></div>\r\n    <div class=\"loadingOverlay\"></div>\r\n</div>\r\n",
  "components/common/modal/modal-close-btn": "<button class=\"modal-close\"\r\n        type=\"button\"\r\n        title=\"{{lang 'common.close'}}\"\r\n        {{additional_attr}}\r\n>\r\n    <span class=\"aria-description--hidden\">{{lang 'common.close'}}</span>\r\n    <span aria-hidden=\"true\">&#215;</span>\r\n</button>\r\n",
  "components/common/alert/alert-error": "<div class=\"alertBox alertBox--error\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span id=\"alertBox-message-text\">{{{this}}}</span>\r\n    </p>\r\n</div>\r\n",
  "components/common/alert/alert-success": "<div class=\"alertBox alertBox--success\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"alertBox-column alertBox-message\">\r\n        <span id=\"alertBox-message-text\">{{{this}}}</span>\r\n    </p>\r\n</div>\r\n"
}